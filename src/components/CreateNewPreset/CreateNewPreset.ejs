<div class="CreateNewPreset js-create-new-preset">
  <div class="CreateNewPresetWrapper">
    <div class="InputDeleteWrapper">
      <div class="InputWrapper">
        <label for="input-delete">
          Type a name for delete preset
        </label>

        <input id="input-delete" class="js-input-delete" />
      </div>

      <button class="CreateNewPreset__DeleteButton js-delete-button">
        Delete
      </button>
    </div>

    <div class="InputWrapper">
      <label for="input">
        Type a name for new preset
      </label>

      <input id="input" class="js-input" required />
    </div>

    <div class="SelectWrapper js-select-wrapper">
      <div>
        Choose options for static
      </div>

      <div class="ContentOptionsWrapper js-content-options-wrapper js-content-static-options-wrapper">
        <!-- checkboxes here are coming from js -->
      </div>
    </div>

    <div class="SelectWrapper js-select-wrapper">
      <div>
        Choose options for dynamic
      </div>

      <div class="ContentOptionsWrapper js-content-options-wrapper js-content-dynamic-options-wrapper">
        <!-- checkboxes here are coming from js -->
      </div>
    </div>

    <div class="SelectWrapper">
      <div class="InputWrapper">
        <label>
          Type resolution
        </label>

        w: <input id="input-w" class="js-input-w" />
        h: <input id="input-h" class="js-input-h" />
      </div>
    </div>

    <div class="SelectWrapper">
      <div class="InputWrapper">
        <label>
          Type width
        </label>

        <input id="input-width" class="js-input-width" />
      </div>
    </div>

    <div class="SelectWrapper js-select-wrapper">
      <div>
        Choose gif formats
      </div>

      <div class="GifFormatsCheckboxesWrapper">
        <div class="GifFormatCheckboxWrapper">
          <label for="gif-formats-sq">
            sq
          </label>

          <input
            type="checkbox"
            checked
            id="gif-formats-sq"
            name="gif-formats"
            class="js-gif-format-checkbox"
            data-id="sq"
          />
        </div>

        <div class="GifFormatCheckboxWrapper">
          <label for="gif-formats-v">
            vertical
          </label>

          <input
            type="checkbox"
            checked
            id="gif-formats-v"
            name="gif-formats"
            class="js-gif-format-checkbox"
            data-id="v"
          />
        </div>

        <div class="GifFormatCheckboxWrapper">
          <label for="gif-formats-h">
            horizontal
          </label>

          <input
            type="checkbox"
            checked
            id="gif-formats-h"
            name="gif-formats"
            class="js-gif-format-checkbox"
            data-id="h"
          />
        </div>
      </div>
    </div>

    <div class="SelectWrapper">
      <div class="InputWrapper">
        <label for="skin-select">
          Choose skin
        </label>

        <select id="skin-select">
          <option value="default" selected>
            default
          </option>

          <option value="cyberpunk">
            cyberpunk
          </option>
        </select>
      </div>
    </div>

    <button class="CreateNewPreset__SubmitButton js-submit-button">
      Submit
    </button>
  </div>
</div>

